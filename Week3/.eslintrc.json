{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "import/extensions": "off",
      "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    }
}
